{
    "security.workspace.trust.untrustedFiles": "open",
    "sqltools.highlightQuery": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.dedupe",
            "label": "dedupe",
            "description": "sort and delete dupes",
            "sequence": [
                "editor.action.sortLinesAscending",
                "editor.action.removeDuplicateLines",
            ]
        }
    ],
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": false,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "fileHistory": false,
        "repositories": true,
        "searchAndCompare": false,
        "launchpad": false
    },
    "gitlens.views.commitDetails.files.layout": "tree",
    "indentRainbow.excludedLanguages": [
        "javascript",
        "typescript",
        "json",
        "html",
        "css",
        "markdown",
        "sql"
    ],
    "application.shellEnvironmentResolutionTimeout": 30,
    "workbench.colorTheme": "Default Light Modern",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Solarized Light",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.multiCursorLimit": 100000,
    "keyword-o-mat.customKeywords": [
        ["True", "false", "true", "False"],
        ["yes", "no"],
        ["on", "off"],
        ["enabled", "disabled"],
        ["active", "inactive"],
        ["success", "failure"],
        ["primary", "secondary"],
        ["default", "custom"],
        ["open", "closed"],
        ["start", "stop"],
        ["up", "down"],
        ["add", "remove"],
        ["create", "delete"],
        ["show", "hide"],
        ["include", "exclude"],
        ["allow", "deny"],
        ["accept", "reject"],
        ["success", "error"],
        ["yes", "no"],
        // ["true", "false"],
        ["enabled", "disabled"],
        ["break", "return", "continue"],
        ["info", "debug", "error"],
        ["INFO", "DEBUG", "ERROR", "WARN"],
    ],
    "pylint.args": [
        "--disable=E0402",
        "--disable=W0702",
        "--disable=E1133",
        "--disable=C0116",
        "--disable=C0301",
        "--disable=C0303",
    ],
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#1e3a8a",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#1e3a8a99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "redhat.telemetry.enabled": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "sortLines.ignoreUnselectedLastLine": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "debug.onTaskErrors": "debugAnyway",
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editor.empty.hint": "hidden",
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false
    },
    "window.commandCenter": false,
    "chat.commandCenter.enabled": false,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "python.analysis.typeCheckingMode": "basic",
    "chat.tools.terminal.autoApprove": {
        "find": true,
        "function": true,
        "}": true,
        "/dev/null": true
    },
    "chat.agent.maxRequests": 100,
    "php.problems.exclude": {
        "/": ["PHP0417", "PHP0413"]
    },
}
