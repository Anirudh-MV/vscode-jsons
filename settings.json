{
    "security.workspace.trust.untrustedFiles": "open",
    "multiCommand.commands": [
        {
            "command": "multiCommand.dedupe",
            "label": "dedupe",
            "description": "sort and delete dupes",
            "sequence": [
                "editor.action.sortLinesAscending",
                "editor.action.removeDuplicateLines",
            ]
        }
    ],
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": false,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "fileHistory": false,
        "repositories": true,
        "searchAndCompare": false,
        "launchpad": false
    },
    "gitlens.views.commitDetails.files.layout": "tree",
    "indentRainbow.excludedLanguages": [
        "javascript",
        "typescript",
        "json",
        "html",
        "css",
        "markdown",
        "sql"
    ],
    "application.shellEnvironmentResolutionTimeout": 30,
    "workbench.colorTheme": "Default Light Modern",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Solarized Light",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.multiCursorLimit": 100000,
    "keyword-o-mat.customKeywords": [
        ["True", "false", "true", "False"],
        ["yes", "no"],
        ["on", "off"],
        ["enabled", "disabled"],
        ["active", "inactive"],
        ["success", "failure"],
        ["primary", "secondary"],
        ["default", "custom"],
        ["open", "closed"],
        ["start", "stop"],
        ["up", "down"],
        ["add", "remove"],
        ["create", "delete"],
        ["show", "hide"],
        ["include", "exclude"],
        ["allow", "deny"],
        ["accept", "reject"],
        ["success", "error"],
        ["yes", "no"],
        // ["true", "false"],
        ["enabled", "disabled"],
        ["break", "return", "continue"],
        ["info", "debug", "error"],
        ["INFO", "DEBUG", "ERROR", "WARN"],
    ],
    "pylint.args": [
        "--disable=E0402",
        "--disable=W0702",
        "--disable=E1133",
        "--disable=C0116",
        "--disable=C0301",
        "--disable=C0303",
    ],
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {},
    "redhat.telemetry.enabled": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "sortLines.ignoreUnselectedLastLine": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
